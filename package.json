{
  "name": "link-lib",
  "version": "1.0.0-beta29",
  "description": "The Link library for creating information based applications",
  "main": "dist/link-lib.umd.js",
  "module": "dist/link-lib.es6.js",
  "sideEffects": false,
  "typings": "dist/typings/link-lib.d.ts",
  "files": [
    "dist/link-lib.es6.js",
    "dist/link-lib.umd.js",
    "dist/typings/**/*.*",
    "package.json",
    "tsconfig.json",
    "yarn.lock"
  ],
  "repository": "https://github.com/fletcher91/link-lib.git",
  "scripts": {
    "build": "rollup -c && tsc -p tsconfig-types.json",
    "doc": "rm -rf dist/docs/* && typedoc --tsconfig tsconfig-types.json --exclude **/*.spec.ts --excludeExternals --excludePrivate --mode file --theme minimal --target ES6 --includeDeclarations --out dist/docs/ src/",
    "doc:deploy": "gh-pages -d ./dist/docs",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prebuild": "rm -rf dist/*",
    "prepublish": "npm run build && npm run doc",
    "postpublish": "npm run doc:deploy",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "author": "Fletcher91 <thom@argu.co>",
  "license": "LGPL-3.0",
  "dependencies": {},
  "peerDependencies": {
    "http-status-codes": ">= 1.x",
    "rdflib": ">= 0.19.x"
  },
  "devDependencies": {
    "@types/firefox-webext-browser": "^67.0.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.7",
    "core-js": "^3.0.1",
    "esdoc": "^1.1.0",
    "gh-pages": "^2.0.1",
    "http-status-codes": ">= 1.x",
    "jest": "^24.7.1",
    "jest-fetch-mock": "^2.1.2",
    "n-quads-parser": "^2.0.2",
    "rdflib": "npm:link-rdflib@0.19.1-20190122T155754",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.4"
  },
  "jest": {
    "automock": false,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/DisjointSet.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "preset": "ts-jest",
    "setupFiles": [
      "core-js",
      "./jest-plugins"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testURL": "http://example.org/resources/5",
    "transform": {
      "\\.ts$": "ts-jest"
    }
  }
}
